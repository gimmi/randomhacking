<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="ExtMvc.Domain.Territory, ExtMvc.Domain" table="`Territories`" schema="`dbo`">

		<id name="TerritoryId" column="`TerritoryID`" type="String" >
			<generator class="assigned" />
		</id>


		<property name="TerritoryDescription" column="`TerritoryDescription`" type="String" not-null="true" />


		<many-to-one name="Region" column="`RegionID`" class="ExtMvc.Domain.Region, ExtMvc.Domain" />


		<set name="Employees" table="`EmployeeTerritories`" schema="`dbo`" inverse="false" cascade="save-update" lazy="true">
			<key column="`TerritoryID`"/>
			<many-to-many class="ExtMvc.Domain.Employee, ExtMvc.Domain" column="`EmployeeID`" />
		</set>

	</class>
</hibernate-mapping>