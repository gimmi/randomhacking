<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="NorthwindWebNHibernate.Business.Order Details, NorthwindWebNHibernate" table="Order Details">
		<id name="OrderID" column="OrderID" type="Int32" unsaved-value="0">
			<generator class="assigned" />
		</id>
		<property name="UnitPrice" column="UnitPrice" type="Decimal" not-null="True" />

		<property name="Quantity" column="Quantity" type="Int16" not-null="True" />

		<property name="Discount" column="Discount" type="Single" not-null="True" />

	</class>
</hibernate-mapping>