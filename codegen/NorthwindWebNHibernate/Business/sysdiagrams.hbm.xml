<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="NorthwindWebNHibernate.Business.sysdiagrams, NorthwindWebNHibernate" table="sysdiagrams">
		<id name="diagram_id" column="diagram_id" type="Int32" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="String" not-null="True" />

		<property name="principal_id" column="principal_id" type="Int32" not-null="True" />

		<property name="version" column="version" type="Int32" not-null="False" />

		<property name="definition" column="definition" type="BinaryBlob" not-null="False" />

	</class>
</hibernate-mapping>